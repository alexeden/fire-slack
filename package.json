{
  "name": "apeden",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "build": "npm run clean && webpack --config webpack.client.config.js",
    "clean": "npm run clean:client && npm run clean:server",
    "clean:client": "rimraf client-dist",
    "clean:server": "rimraf server-dist",
    "build:watch-client": "npm run clean:client && nodemon --exec \"webpack --config webpack.client.config.js\" -w webpack.client.config.js",
    "build:watch-server": "npm run clean:server && nodemon --exec \"webpack\" -w webpack.server.config.js",
    "start": "webpack-dev-server --inline --config ./webpack.client.config.js",
    "start:server": "npm run clean:server && nodemon -e ts,json -w server --exec \"webpack --config webpack.server.config.js && node server-dist/server.js\""
  },
  "author": "Alex Eden",
  "dependencies": {
    "@angular/animations": "^4.1.2",
    "@angular/common": "^4.1.2",
    "@angular/compiler": "^4.1.2",
    "@angular/compiler-cli": "^4.1.2",
    "@angular/core": "^4.1.2",
    "@angular/forms": "^4.1.2",
    "@angular/http": "^4.1.2",
    "@angular/platform-browser": "^4.1.2",
    "@angular/platform-browser-dynamic": "^4.1.2",
    "@angular/router": "^4.1.2",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "firebase": "^3.9.0",
    "firebase-admin": "^4.2.1",
    "moment": "^2.18.1",
    "ramda": "^0.23.0",
    "rxjs": "^5.4.0",
    "tslib": "^1.7.1",
    "web-animations-js": "^2.2.5",
    "whatwg-fetch": "^2.0.3",
    "zone.js": "^0.8.10"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.41",
    "@types/node": "^7.0.14",
    "@types/ramda": "0.0.7",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.1.3",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "circular-dependency-plugin": "^3.0.0",
    "codelyzer": "^3.0.1",
